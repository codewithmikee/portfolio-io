{
  "name": "@workspace/eslint-config",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./base": "./base.js",
    "./next-js": "./next.js",
    "./react-internal": "./react-internal.js"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:lint",
    "@typescript-eslint/parser": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-config-prettier": "catalog:lint",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "catalog:lint",
    "eslint-plugin-react-hooks": "catalog:lint",
    "eslint-plugin-turbo": "catalog:lint",
    "globals": "catalog:lint",
    "typescript": "catalog:types",
    "typescript-eslint": "catalog:"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rimraf dist",
    "build": "echo 'No build step'"
  }
}
